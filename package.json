    "jsonwebtoken": "9.0.2",
    "minio": "^7.1.3",
    "mongoose": "^7.5.3",
    "multer": "1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.13.0",
    "resend": "^2.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
